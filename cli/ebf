  echo "  set_mode_mask          1-255                  0-255"
  echo "  get_mode_mask          1-255                           "
  echo "  write_mask             1-255                  0-255    "
  echo "  read_mask              1-255                           "
  echo "  set_mode               1-8                    {read,write}"
  echo "  get_mode               1-8                                "
  echo "  read                   1-8                                "
  echo "  write                  1-8                    0-1         "
}

