	DEVICE=$OPTION
	DIR_PATH=$OPTION4
	OUTPUT=$(curl -s -k --location --request POST "$SERVER_URL/api/v0.2/devices/$DEVICE/sdcard/fmanager/dir/$DIR_PATH"  --header 'Authorization: token '$AUTH_TOKEN'')
