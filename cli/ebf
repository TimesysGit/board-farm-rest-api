function help_device_hotplug() {
  echo " "
  echo "Usage: ebf <device name> hotplug [1-4] [status/on/off/switch]"
  echo ""
	echo "Example: ebf bbb01 hotplug 1 status"
  echo ""
  echo "Options:"
  echo "   status                  Provides the current Hotplug status whether it is"
  echo "                           connected to devise side or not"
  echo "   on                      Connect hotplug to Device side"
  echo "   off                     Disconnect hotplug from Device"
  echo "   switch                  Toggle the current state of a particular hotplug"
  echo ""
  exit
}

function help_device_portfw() {
  echo " "
  echo "Usage: ebf <device name> portfw [list/add/remove] [NAT/SSH]"
  echo ""
	echo "Examples:"
	echo "    1. ebf bbb01 portfw list ssh (List all the SSH forwarded port)"
	echo "    2. ebf bbb01 portfw add ssh 192.168.111.10 root root 22 8022 (For SSH port forward)"
	echo "    3. ebf bbb01 portfw remove ssh 8022 (For removing SSH forwarding for a Zombie port)"
  echo ""
	echo ""
  echo "Options:"
  echo "   list [NAT/SSH]               Provides list of all the IP rules forwarded on a Zombie"
  echo "   add  [NAT/SSH] [ARGS]        Add a new NAT/SSH rule"
  echo "   remove  [NAT/SSH] [ARGS]     Remove a NAT/SSH rule"
  echo ""
	echo ""
	echo "Run 'ebf  <device name> portfw [OPTION] help' for more information of a option."
	echo ""
  exit
}

function help_device_portfw_list() {
  echo " "
  echo "Usage: ebf <device name> portfw list [NAT/SSH]"
  echo ""
	echo "Examples:"
	echo "    1. ebf bbb01 portfw list ssh (List all the SSH forwarded rules)"
	echo "    2. ebf bbb01 portfw list nat (List all the NAT forwarded riles)"
  echo ""
  echo "Options:"
  echo "   ssh      List all the SSH forwarded rules"
  echo "   nat      List all the NAT forwarded riles"
  echo ""
  exit
}

function help_device_portfw_add() {
	echo " "
	echo "For SSH Forward"
	echo " Usage  : ebf <device name> portfw add ssh <device-ip> <username> <password> <dut-port> <zombie-port>"
	echo " Example: ebf bbb01 portfw add ssh 192.168.111.10 root root 22 8022"
	echo ""
	echo ""
	echo "For NAT Forward"
	echo " Usage  : ebf <device name> portfw add nat <device-ip> <dut-port> <zombie-port> <protocol>"
	echo " Example: ebf bbb01 portfw add nat 192.168.111.10 22 8022 tcp"
  echo ""
	echo ""
  exit
}

function help_device_portfw_remove() {
	echo " "
	echo "For Removing SSH Forward Rule"
	echo " Usage  : ebf <device name> portfw remove ssh <zombie-port>"
	echo " Example: ebf bbb01 portfw remove ssh 8022"
	echo ""
	echo ""
	echo "For Removing NAT Forward Rule"
	echo " Usage  : ebf <device name> portfw remove nat <device-ip> <dut-port> <zombie-port> <protocol>"
	echo " Example: ebf bbb01 portfw remove nat 192.168.111.10 22 8022 tcp"
  echo ""
	echo ""
	exit 1
}

